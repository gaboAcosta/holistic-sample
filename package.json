{
  "name": "holistic-sample",
  "version": "1.0.0",
  "description": "Desired output of the holistic js scaffolding",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev:up": "docker-compose -f docker-compose.yaml -f docker-compose.local.yaml up --build",
    "dev:down": "docker-compose -f docker-compose.yaml -f docker-compose.local.yaml down",
    "test:main": "docker-compose run main test",
    "test:web": "docker-compose run web test",
    "e2e": "yarn e2e:build && yarn e2e:run",
    "e2e:run": "docker-compose -f docker-compose.yaml -f docker-compose.e2e.yaml run --rm nightwatch",
    "e2e:build": "docker-compose -f docker-compose.yaml -f docker-compose.e2e.yaml build",
    "e2e:down": "docker-compose -f docker-compose.yaml -f docker-compose.e2e.yaml down"
  }
}
